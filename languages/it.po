#: ItemDuplicator.php:12
msgid "Duplicate"
msgstr "Duplica"

#: config_form.php:9
msgid "Allowed users"
msgstr "Utenti autorizzati"

#: config_form.php:13
msgid "Restrict privilege"
msgstr "Restrizione privilegi"

#: config_form.php:17
msgid "If checked, only users with Super User or Admin role will be able to duplicate Items."
msgstr "Se selezionata, solo gli utenti con ruolo Super User o Admin saranno in grado di duplicare elementi."

#: config_form.php:23
msgid "Information removal"
msgstr "Rimozione informazioni"

#: config_form.php:28
msgid "Empty Title field"
msgstr "Svuota campo Titolo"

#: config_form.php:32
msgid "If checked, field DublinCore:Title will be emptied when showing duplicate Item."
msgstr "Se selezionata, il campo Titolo (DublinCore:Title) verrà svuotato alla visualizzazione dell'elemento duplicato."

#: config_form.php:40
msgid "Svuota campo Soggetto"
msgstr ""

#: config_form.php:44
msgid "If checked, field DublinCore:Subject will be emptied when showing duplicate Item."
msgstr "Se selezionata, il campo Soggetto (DublinCore:Subject) verrà svuotato alla visualizzazione dell'elemento duplicato."

#: config_form.php:52
msgid "Svuota campo Data"
msgstr ""

#: config_form.php:56
msgid "If checked, field DublinCore:Date will be emptied when showing duplicate Item."
msgstr "Se selezionata, il campo Data (DublinCore:Date) verrà svuotato alla visualizzazione dell'elemento duplicato."

#: config_form.php:64
msgid "Remove Tags"
msgstr "Rimozione Tag"

#: config_form.php:68
msgid "If checked, Tags will be removed when showing duplicate Item."
msgstr "Se selezionata, i <i>tag</i> verranno rimossi alla visualizzazione dell'elemento duplicato."